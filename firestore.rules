rules_version = '2';

service cloud.firestore {
  match /databases/{database}/documents {
    match /blogs/{blogId} {
      allow read;
    }
    match /blogs/{blogId}/sections/{sectionId} {
      allow read;
    }
    match /books/{bookId} {
      allow read;
      //allow write: if request.auth.uid != null
    }
    match /users/{userId} {
      allow read: if request.auth.uid == userId;

    }
  }
}